function _update()
end
function _draw()
cls()
map()
local x,y=10,39
rectfill(x,y,x+108,y+42,0)
x+=3 y+=3
rectfill(x,y,x+102,y+36,5)
x+=2 y+=4
cursor(x,y)
print("paulus' first year review",7)
print("")
x+=14 y+=12 --35x35
cursor(x,y)
print("░",13)
x+=8
cursor(x,y)
print("gender",14)
x+=24 
cursor(x,y)
print("░",13)
x+=8
cursor(x,y)
print("games",12)
x+=20
cursor(x,y)
print("░",13)
print("")
x=15 y=y+12
cursor(x,y)
print("  april 28th @ 10:00 am",7)

end
